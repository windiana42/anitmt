  // *********************
  // to_angle
  // *********************
  one_operand_operator to_angle
  {
    calc_result( angle ) 
    {
      int quotient = int(angle) / 360;
      scalar res = angle - quotient * 360;
      if( res < 0 ) res += 360;
      return res;
    }
    versions
    {
      scalar to_angle( scalar );
    }
  }
  
