  {
    calc_result( val ) 
    {
      return sqrt(val);
    }
    is_operand_ok( op, info )
    {
      // value under square root must be positive
      if( op < 0 )
      {
	if( !info->is_trial_run() )
	  error() << "cannot calculate square root of " << op;
	return false;
      }
      return true;
    }
    versions
    {
      scalar sqrt( scalar );
    }
  }

